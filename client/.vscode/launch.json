    "version": "0.2.0",
    "configurations": [
           {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "pwa-chrome",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}",
        "runtimeExecutable": "/snap/bin/chromium",
        "runtimeArgs": ["--new-Window", "--remote-debugging-port=9222", "--disable-background-networking","-user-data-dir=\"/${workspaceFolder}/log"]
    },
    ]
}